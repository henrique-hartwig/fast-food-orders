{
  "name": "fast-food-orders",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "start:local": "serverless offline start --stage local",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "migrate": "knex migrate:latest",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "build:lambda": "rm -rf .build && tsc && cp -r node_modules .build/node_modules",
    "package:lambda": "./package-lambda.sh",
    "deploy:infra": "npm run build:lambda && npm run package:lambda && terraform -chdir=terraform apply"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.400.0",
    "@prisma/client": "^6.6.0",
    "aws-lambda": "^1.0.7",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.9",
    "@types/pg": "^8.10.2",
    "@types/uuid": "^9.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "serverless": "^3.34.0",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}